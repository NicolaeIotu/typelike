{
  "name": "typelike",
  "version": "1.0.3",
  "type": "module",
  "description": "Deep type checking by comparison",
  "main": "dist/typelike.js",
  "scripts": {
    "preversion": "npm -y exec -- standard --fix lib/* && npm test && npm -y exec -- terser lib/typelike.js -c --comments --keep-fnames > dist/typelike.js",
    "version": "git add -A dist",
    "postversion": "git push --set-upstream-to && git push --tags",
    "standard": "npm -y exec -- standard lib/*",
    "coverage": "node --experimental-test-coverage test/main.js coverage",
    "test": "node --experimental-test-coverage test/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolaeIotu/typelike.git"
  },
  "keywords": [
    "typelike",
    "types",
    "checks",
    "compare",
    "deep",
    "checking",
    "comparison"
  ],
  "author": {
    "email": "nicolae.g.iotu@gmail.com",
    "name": "Nicolae, Iotu",
    "url": "https://github.com/NicolaeIotu"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NicolaeIotu/typelike/issues"
  },
  "homepage": "https://github.com/NicolaeIotu/typelike#readme"
}
